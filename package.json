{
  "name": "v1",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "mocha --exit --timeout 5000 -r ts-node/register -r dotenv/config test/**/*.ts",
    "testDebug": "mocha --exit -r ts-node/register -r dotenv/config --inspect=5858 test/**/*.ts",
    "dev": "node -r ts-node/register -r dotenv/config ./src/app.ts",
    "dev:watch": "nodemon -r dotenv/config ./src/app.ts",
    "devDebug": "node --inspect=5858 -r ts-node/register ./src/app.ts",
    "start": "tsc && node -r dotenv/config ./build/app.js",
    "build": "tsc",
    "devNo-tsnode": "concurrently \"tsc -w\" \"nodemon ./build/app.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/**/*",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/winston": "^2.4.4",
    "apollo-server-express": "^2.19.0",
    "basic-auth": "^2.0.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-winston": "^4.0.3",
    "geojson-utils": "^1.1.0",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.11.1",
    "tsscmp": "^1.0.6",
    "typescript": "^3.7.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chai-spies": "^1.0.2",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.14",
    "@types/node": "^13.1.4",
    "@types/node-fetch": "^2.5.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2"
  }
}
